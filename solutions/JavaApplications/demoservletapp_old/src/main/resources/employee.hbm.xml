<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tfl.models.Employee" table="employees">
		<id column="id" name="id" type="java.lang.Integer" />
		<property column="firstname" name="userName" type="java.lang.String" />
		<property column="lastname" name="password1" type="java.lang.String" />
	</class>
</hibernate-mapping>